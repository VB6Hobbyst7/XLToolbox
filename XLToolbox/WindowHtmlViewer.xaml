<Window x:Class="XLToolbox.WindowHtmlViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:l="clr-namespace:XLToolbox"
        Width="640" Height="480"
        Title="{x:Static l:Strings.XLToolboxLicense}">
    <Window.Resources>
        <ResourceDictionary Source="Style.xaml" />
    </Window.Resources>
    <DockPanel LastChildFill="True" Margin="10">
        <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right"
                    Margin="0,10,0,0">
            <Button Name="ButtonClose" Content="{x:Static l:Strings.Close}" IsCancel="True" Click="ButtonClose_Click" />
        </StackPanel>
        <WebBrowser DockPanel.Dock="Top" Name="Browser" />
    </DockPanel>
</Window>
